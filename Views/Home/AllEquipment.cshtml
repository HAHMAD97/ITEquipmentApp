@model IEnumerable<ITEquipmentBorrowApp.Models.ITEquipment>
<div>
    <h2>All Equipment</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Equipment Id</th>
                <th>Type</th>
                <th>Description</th>
                <th>Availability</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var equipment in Model)
            {
                <tr>
                    <td>@equipment.EquipmentId</td>
                    <td>@equipment.Type</td>
                    <td>@equipment.Description</td>
                    <td>@equipment.Availability</td>
                </tr>
            }
        </tbody>
    </table>
</div>

               